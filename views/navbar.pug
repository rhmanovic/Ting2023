        a.navbar-brand.nav-item.m-0.p-0.col-auto(href="/")
          img.udemy-logo.mx-2.hideMIN(src='https://itcstore.net/img/upload/myFile-1681091679923.png', alt='Mawadi', data-purpose='udemy-brand-logo', height='45')

        //- ul.navbar-nav.mr-auto
        .col
          label.sr-only(for='inlineFormInputGroup') بحث
          .input-group
            input#inlineFormInputGroup.form-control.border-left-0(type='text', placeholder='بحث')
            .input-group-prepend
              .input-group-text.tomato.text-white
                i.fa.fa-search(aria-hidden='true')


        .navbar-nav.mx-4.mr-md-0
          a.nav-item.nav-link.mx-4(href='/')
            i.fa.fa-user.mt-1(aria-hidden='true')
          a.nav-item.nav-link.mx-4(href='/cart')
            i.fa.fa-shopping-cart.mt-1(aria-hidden='true')
          
          .nav-item
            if currentUser
              div
                img.avatar.img-circle.hidden-xs-downborder.border.collapse(src='/images/avatar.png', alt='avatar')
                a.btn.pull-md-left.btn-secondary(href='/logout') Log out
            else  
              a.btn.btn-info.pull-md-left.mr-md-2.collapse(href='/login')
                i.fa.fa-user.ml-2(aria-hidden='true')
                span تسجيل دخول
              //- a.btn.btn-outline-danger.pull-md-left(href='/register') Sign Up
    .col.menu.navbar-collapse.collapse.border-bottom.border-3.shadowUnder
